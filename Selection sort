#include<stdio.h>
int main()
{
	int i,j,position,swap;
	int arr[10]={13,35,43,67,8,6,9,12,32,56};
	int n=10;
	for(i=0;i<(n-1);i++)
	{
		position=i;
		for(j=i+1;j<n;j++)
		{
			if(arr[position]>arr[j])
			    position=j;
		}
		if(position!=i)
		{
			swap=arr[i];
			arr[i]=arr[position];
			arr[position]=swap;
		}
	}
	for(i=0;i<n;i++)
	printf("%d\t",arr[i]);
	return 0;
	
}
